<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="eda397.group10.navigator"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-permission android:name="android.permission.INTERNET" />

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="eda397.group10.navigator.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="eda397.group10.navigator.ProjectPageActivity"
            android:label="@string/title_activity_project_page" >
        </activity>
        <activity 
            android:name="eda397.group10.navigator.AuthenticatedMainActivity">
        </activity>
        
        <service android:name="eda397.group10.notifications.EventService" />
    </application>

</manifest>
