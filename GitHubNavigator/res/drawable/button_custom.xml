<selector xmlns:android="http://schemas.android.com/apk/res/android">
 <item android:state_pressed="true" >
  <shape>
  <solid
   android:color="#ef4444" />
  <stroke
   android:width="1dp"
   android:color="#FFFFFF" />
  <corners
   android:radius="3dp" />
  <padding
   android:left="10dp"
   android:top="10dp"
   android:right="10dp"
   android:bottom="10dp" />
  </shape>
 </item>
  <item>
  <shape>
  <gradient
   android:startColor="#ff4000"
   android:endColor="#992f2f"
   android:angle="270" />
  <stroke
   android:width="1dp"
   android:color="#FFFFFF" />
  <corners
   android:radius="3dp" />
  <padding
   android:left="10dp"
   android:top="10dp"
   android:right="10dp"
   android:bottom="10dp" />
 </shape>
 </item>
</selector>
